<%= render "core/navbar_generic" %>
<div class="banner page-banner d-flex align-items-center" style="height: 30vh">
  <div class="container">
    <h1 class="mb-3 banner-h1-sm">Add a new <span>List!</span></h1>
  </div>
</div>
<div class="container-mid">
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
  <%= link_to "Back", lists_path %>
</div>
<div class="footer-banner"></div>
